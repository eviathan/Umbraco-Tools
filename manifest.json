{
  "manifest_version": 2,

  "name": "Umbraco tools",
  "description": "A collection of tools for Umbraco",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
      "scripts": ["jquery.min.js", "popup.js"]
  }
}
